<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle%></h1>
    </div>
    <div class="box">
        <% for(var i = 0; i<todoItems.length; i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= todoItems[i]%></p> 
            </div>
        <% } %>
        <form action="/" method="POST" class="item">
            <input type="text" placeholder="ToDo..." name="todoItem" autocomplete="off"/>
            <button type="submit" name="list" value=<%= listTitle%>>+</button>
          </form>
    </div>
<%- include("footer")-%>